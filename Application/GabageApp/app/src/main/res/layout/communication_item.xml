<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/communication_item_header"
        android:layout_width="0dp"
        android:layout_height="91dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.235"
        app:srcCompat="@color/statusBar" />

    <ImageButton
        android:id="@+id/communication_item_delete_button"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/communication_name_text"
        app:srcCompat="@android:drawable/ic_delete" />

    <TextView
        android:id="@+id/configuration_id_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Communication Id"
        app:layout_constraintBottom_toBottomOf="@+id/communication_item_header"
        app:layout_constraintEnd_toEndOf="@+id/communication_item_header"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="@+id/communication_item_header"
        app:layout_constraintTop_toTopOf="@+id/communication_item_header"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/communication_id_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="0"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/configuration_id_label"
        app:layout_constraintEnd_toEndOf="@+id/communication_item_header"
        app:layout_constraintHorizontal_bias="0.169"
        app:layout_constraintStart_toEndOf="@+id/configuration_id_label"
        app:layout_constraintTop_toTopOf="@+id/configuration_id_label" />

    <TextView
        android:id="@+id/communication_name_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Communication Name"
        app:layout_constraintBottom_toBottomOf="@+id/communication_item_header"
        app:layout_constraintEnd_toEndOf="@+id/communication_item_header"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="@+id/communication_item_header"
        app:layout_constraintTop_toBottomOf="@+id/configuration_id_label"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/communication_name_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="N/D"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/communication_name_label"
        app:layout_constraintEnd_toEndOf="@+id/communication_item_header"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toEndOf="@+id/communication_name_label"
        app:layout_constraintTop_toTopOf="@+id/communication_name_label" />

    <TextView
        android:id="@+id/communication_value_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="Value"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/communication_item_delete_button"
        app:layout_constraintHorizontal_bias="0.143"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/communication_item_header"
        app:layout_constraintVertical_bias="0.68" />

    <TextView
        android:id="@+id/communication_value_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="0"
        app:layout_constraintBottom_toBottomOf="@+id/communication_value_label"
        app:layout_constraintEnd_toStartOf="@+id/communication_item_delete_button"
        app:layout_constraintStart_toEndOf="@+id/communication_value_label"
        app:layout_constraintTop_toTopOf="@+id/communication_value_label" />

</android.support.constraint.ConstraintLayout>